package com.wissen.array;

import java.util.*;
import java.util.Map.Entry;

public class DuplicateElementSameNumberOfTimes {

	public boolean isDuplicateElementsSameNoOfTimes(int[] nums) {
		
	}
	
	public static void main(String[] args) {
		int[] nums = {1,2,3,4,2,3,4,3,4,4,5,5,10,2,3};
		int[] nums2 = {1,2,3,4,2,3,4,1};
		Map<Integer, Integer> hmap = new HashMap<>();
		Set<Integer> set= new HashSet();
		
		for(int i=0; i<nums.length; i++) {
			hmap.put(nums[i], hmap.getOrDefault(nums[i], 0)+1);
		}
		
		for(Entry<Integer, Integer> map : hmap.entrySet()) {
			int key = map.getKey();
			int val = map.getValue();
			if(set.contains(val) == true) {
				System.out.println(false);
				break;
			}
			else {
				set.add(val);
			}
		}
		if(set.size() == hmap.size()) {
			System.out.println(true);
		}

	}

}
