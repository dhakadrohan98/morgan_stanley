package com.wissen.multithreading;

public class RunnableInterfaceExample implements Runnable {

	@Override
	public void run() {
		for(int i=0; i<10; i++) {
			System.out.println(Thread.currentThread().getId() + " " + i);
			try {
				Thread.sleep(500);
			} catch (InterruptedException e) {
				e.printStackTrace();
			}
		}
		
	}
	
	public static void main(String[] args) {
		RunnableInterfaceExample runn = new RunnableInterfaceExample();
	}
	
}
