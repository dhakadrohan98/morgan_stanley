package com.wissen;

public class SubsequenceOfString {

	static StringBuilder sb = new StringBuilder();
	public static int longestPalindromeSubseq(String s) {
        int i = 0;
        String str = "";
        helper(sb, s, i);
        return 0;
    }
	
	public static void helper(StringBuilder sb, String orginal, int i) {
        //base case
        if(i >= orginal.length()) {
            return;
        }
        
        //pick
       sb.append(orginal.charAt(i));
       helper(sb,orginal,i+1);
        //not pick
        helper(sb,orginal,i+1);
        
    }
	
	public static void main(String[] args) {
		String s = "bbbab";
		longestPalindromeSubseq(s);

	}

}
