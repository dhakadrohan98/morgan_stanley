package com.wissen;

public class MoveZeroesToEnd {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}
	
	
	//Naive approach
	
	//TC: O(n^2)
    //SC: O(1)
    public void moveZeroes1(int[] nums) {
        int n = nums.length;
        
        for(int i=0; i<n; i++) {
            if(nums[i] == 0) {
                for(int j=i+1; j<n; j++) {
                    if(nums[j] != 0) {
                        swap(nums, i, j);
                        break;
                    }
                }
            }
        }
    }
    
    private void swap(int[] nums, int i, int j) {
        int temp = nums[i];
        nums[i] = nums[j];
        nums[j] = temp;
    }

}
