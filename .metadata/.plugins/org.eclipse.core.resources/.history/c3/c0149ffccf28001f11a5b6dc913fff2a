package com.wissen.priorityqueue;

import java.util.*;

public class MinHeap {

	public static void main(String[] args) {
		Queue<Integer> pq = new PriorityQueue<>();
		
		pq.add(10);
		pq.add(8);
		pq.add(9);
		pq.add(4);
		pq.add(5);
		pq.add(2);
		pq.add(3);
		
		for(Integer val : pq) {
			System.out.print(pq.poll()+ " ");
		}
	}

}

