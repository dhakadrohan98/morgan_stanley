package com.wissen;

public class Test {

	public static void main(String[] args) {
		int sum = 5+3;
		System.out.println(sum);
		
		//fibonacci series within given range (4,9)
		int start = 4;
		int end = 9;
		int first = 0;
		int second = 1;
		int sum = first + second;
		
		while(sum <= end) {
			if(sum >= start) {
				System.out.println(sum);
			}
			first = second;
			second = sum;
			sum = first+sum;
		}

	}

}
