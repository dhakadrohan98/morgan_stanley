package com.wissen;

import java.util.*;

public class PrintCharactersFreq {

	public void printCharacterFreq(String str) {
		LinkedHashMap<Character, Integer> hmap = new LinkedHashMap<>();
		for (int i = 0; i < str.length(); i++) {
			char ch = str.charAt(i);
			hmap.put(ch, );
		}
	}

