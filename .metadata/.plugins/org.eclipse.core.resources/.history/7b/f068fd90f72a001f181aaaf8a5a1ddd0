package com.wissen;

import java.util.*;

public class SubsequenceOfString {

	public static int longestPalindromeSubseq(String s) {
        int i = 0;
        String result = "";
        generateSubsequences(s, i, result);
        return 0;
    }
	
	public static List<String> generateSubsequences(String orginal, int i, String result) {
		List<String> subsequences = new ArrayList<String>();
        //base case
        if(i >= orginal.length()) {
        	subsequences.add(result);
        	return subsequences;
        }
        
        //pick
       result += orginal.charAt(i) + helper(result,orginal,i+1);
       //not pick
       return result += helper(result,orginal,i+1);
        
    }
	
	public static void main(String[] args) {
		String s = "bbbab";
		longestPalindromeSubseq(s);
		System.out.println(sb.toString());

	}

}
