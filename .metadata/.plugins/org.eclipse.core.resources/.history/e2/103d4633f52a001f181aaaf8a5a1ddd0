package com.wissen;

public class SubsequenceOfString {

	public int longestPalindromeSubseq(String s) {
        int i = 0;
        String str = "";
        System.out.println(helper(str, str, i));
        
    }
	
	public static String helper(String str, String orginal, int i) {
        //base case
        if(i < orginal.length()) {
            return str;
        }
        
        //pick
        str = str + orginal.charAt(i) + helper(str,orginal,i+1);
        //not pick
        str = str + helper(str,orginal,i+1);
        return str;
        
    }
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
