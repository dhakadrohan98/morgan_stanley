package com.wissen;

public class SubsequenceOfString {

	public int longestPalindromeSubseq(String s) {
        int i = 0;
        
    }
	
	public String helper(String str, String orginal, int i) {
        //base case
        if(i < 0) {
            return str;
        }
        
        //pick
        str = str + orginal.charAt(i) + helper(str,orginal,i+1);
        //not pick
        str = str + helper(str,orginal,i+1);
        return str;
        
    }
	
