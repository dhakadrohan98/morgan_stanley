package com.wissen;

public class SubsequenceOfString {

	static StringBuilder sb = new StringBuilder();
	public static int longestPalindromeSubseq(String s) {
        int i = 0;
        String result = "";
        helper(result, s, i);
        return 0;
    }
	
	public static String helper(String result, String orginal, int i) {
        //base case
        if(i >= orginal.length()) {
        	result);
        }
        
        //pick
       result += orginal.charAt(i) + helper(result,orginal,i+1);
        //not pick
        helper(sb,orginal,i+1);
        
    }
	
	public static void main(String[] args) {
		String s = "bbbab";
		longestPalindromeSubseq(s);
		System.out.println(sb.toString());

	}

}
